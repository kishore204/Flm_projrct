<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" 	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:html="http://www.w3.org/1999/xhtml">
    <Dialog title="Example" contentWidth="90%" contentHeight="90%" 
            horizontalScrolling="false" verticalScrolling="false" class="iframeHolder">
    	
    	
    	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormChange354"
			minWidth="1024"
			maxContainerCols="2"
			editable="true"
			layout="ResponsiveGridLayout"
			title="Address"
			labelSpanL="3"
			labelSpanM="3"
			emptySpanL="4"
			emptySpanM="4"
			columnsL="1"
			columnsM="1"
			class="editableForm">
			<f:content>
				<Label text="Name" />
				<Input id ="Name" change = "Name" />
					<Label text="AdharNumber" />
				<Input id ="Adhar" change = "mobile" type = "Number" />
				<Label text="Address" />
				<Input id="Street" >
				</Input>
			
				<Label text="City" />
			
				<Input id="City" change = "Name"  />
				<Label text="Gender" />
				<Select width="100%" id = 'gen'>
					<items>
						<core:Item text="Male" />
						<core:Item text="Female" />
					</items>
				</Select>
			</f:content>
		</f:SimpleForm>
	</VBox>
        <!--<html:iframe src="https://www.google.co.in/" height="100%" width="100%"/>-->
        <buttons>
        		<Button type="Accept" text = 'Save Details' press = 'save'></Button>
            <Button text="close" press="closeDialog"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>